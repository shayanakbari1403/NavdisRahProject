{{> page_menu }}

       <div class="main-content">
            <!-- Breadcrumbs Start -->
            <div class="rs-breadcrumbs sec-color">
                <div class="breadcrumbs-image">
                    <div class="dark-layer"></div>
                    <img src="images/breadcrumbs/contact.jpg" class="contact-image" alt="Breadcrumbs Image">
                    <div class="breadcrumbs-inner">
                        <div class="container">
                            <div class="breadcrumbs-text">
                                <h1 class="breadcrumbs-title">{{title}}</h1>
                            </div>
                        </div>
                    </div>
                </div>                
            </div>
            <!-- Breadcrumbs End -->            

            <!-- Contact Start -->
            <section id="rs-contact" class="rs-contact contact-section gray-color pb-100">
                <!-- About Icon Start -->
                <div id="rs-about-icon" class="rs-about-icon pt-100">
                    <div class="container">
                        <div class="icon-section">
                            <div class="single-icon">
                                <div class="icon-part">
                                <div class="icon-text">
                                    <a dir="ltr" class="icon-info" href="tel:{{phone}}">{{phone}}</a>
                                </div>
                                    <a href="#"><i class="fa fa-phone"></i></a>
                                </div>
                            </div>
                            <div class="single-icon">
                                <div class="icon-part">
                                <div id="email-address" class="icon-text">
                                    {{!-- <a  class="icon-info" href=""></a> --}}
                                </div>
                                    <a href="#"><i class="fa fa-envelope-o"></i></a>
                                </div>
                            </div>
                            <div class="single-icon">
                                <div class="icon-part">
                                <div class="icon-text">
                                    <p>{{{fax}}}</p>
                                </div>
                                    <a href="#"><i class="fa fa-fax"></i></a>
                                </div>
                            </div>
                            <div class="single-icon margin-0">
                                <div class="icon-part">
                                <div class="icon-text after-none">
                                    <p>{{address}}</p>
                                </div>
                                    <a href="#"><i class="fa fa-map-marker"></i></a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- About Icon End -->
            </section>
            <!-- Contact End -->

            <!-- Map Section Start -->
            <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d6664.909874651978!2d51.41175268890511!3d35.7609684496626!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x3f8e0693d41c9fbf%3A0x1aa5e9144cbfc801!2sNavdis+Rah+Co.!5e0!3m2!1sen!2sus!4v1545857392423" width="100%" height="500" frameborder="0" style="border:0" allowfullscreen></iframe>
            {{!-- <section id="rs-map" class="rs-map">
                <div id="googleMap"></div>
            </section> --}}
            <!-- Map Section End -->          
        </div> 
        <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCDFU1EPE4ivwBibRlCPJKlnnLcpaWSXzM"></script>
        <script src="/js/email.js"></script>
<script>
    var address= mail2("info","navdisrah",0,"");
    var email = document.getElementById('email-address');
    email.innerHTML =address;
    email.children[0].className = 'icon-info';
    //email.href = 'mailto:'+address; 
</script>